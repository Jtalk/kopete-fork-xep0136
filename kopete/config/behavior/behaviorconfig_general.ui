<!DOCTYPE UI><UI version="3.2" stdsetdef="1">
<class>BehaviorConfig_General</class>
<widget class="QWidget">
    <property name="name">
        <cstring>BehaviorConfig_General</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>422</width>
            <height>400</height>
        </rect>
    </property>
    <property name="caption">
        <string>General</string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QCheckBox">
            <property name="name">
                <cstring>mUseQueueChk</cstring>
            </property>
            <property name="text">
                <string>Use the message &amp;queue</string>
            </property>
            <property name="accel">
                <string>Alt+Q</string>
            </property>
            <property name="whatsThis" stdset="0">
                <string>Use a queue instead of opening a chat window on incoming messages</string>
            </property>
        </widget>
        <widget class="QButtonGroup">
            <property name="name">
                <cstring>buttonGroup2</cstring>
            </property>
            <property name="title">
                <string>System Tray</string>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QCheckBox">
                    <property name="name">
                        <cstring>mShowTrayChk</cstring>
                    </property>
                    <property name="text">
                        <string>Show the system tray &amp;icon</string>
                    </property>
                    <property name="accel">
                        <string>Alt+I</string>
                    </property>
                </widget>
                <widget class="QCheckBox">
                    <property name="name">
                        <cstring>mStartDockedChk</cstring>
                    </property>
                    <property name="text">
                        <string>Start with the contact list &amp;minimised to the system tray</string>
                    </property>
                    <property name="accel">
                        <string>Alt+M</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Start with the main window hidden, the only visible item is the system tray icon</string>
                    </property>
                </widget>
            </vbox>
        </widget>
        <widget class="QButtonGroup">
            <property name="name">
                <cstring>notifyGroupBox</cstring>
            </property>
            <property name="title">
                <string>&amp;Notifications</string>
            </property>
            <grid>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QCheckBox" row="0" column="0">
                    <property name="name">
                        <cstring>mBalloonNotifyChk</cstring>
                    </property>
                    <property name="text">
                        <string>Show a &amp;bubble on new message</string>
                    </property>
                </widget>
                <widget class="QCheckBox" row="1" column="0">
                    <property name="name">
                        <cstring>mTrayflashNotifyChk</cstring>
                    </property>
                    <property name="text">
                        <string>Flash the system &amp;tray on new messages</string>
                    </property>
                </widget>
                <widget class="QCheckBox" row="2" column="0">
                    <property name="name">
                        <cstring>mSoundIfAwayChk</cstring>
                    </property>
                    <property name="text">
                        <string>Enable events while away</string>
                    </property>
                </widget>
                <widget class="QPushButton" row="3" column="0">
                    <property name="name">
                        <cstring>configSound</cstring>
                    </property>
                    <property name="text">
                        <string>Con&amp;figure Sounds &amp;&amp; Events...</string>
                    </property>
                </widget>
                <spacer row="3" column="1">
                    <property name="name">
                        <cstring>spacer2</cstring>
                    </property>
                    <property name="orientation">
                        <enum>Horizontal</enum>
                    </property>
                    <property name="sizeType">
                        <enum>Expanding</enum>
                    </property>
                    <property name="sizeHint">
                        <size>
                            <width>8</width>
                            <height>16</height>
                        </size>
                    </property>
                </spacer>
            </grid>
        </widget>
        <spacer>
            <property name="name">
                <cstring>spacer1</cstring>
            </property>
            <property name="orientation">
                <enum>Vertical</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>16</width>
                    <height>8</height>
                </size>
            </property>
        </spacer>
    </vbox>
</widget>
<tabstops>
    <tabstop>mBalloonNotifyChk</tabstop>
    <tabstop>mTrayflashNotifyChk</tabstop>
    <tabstop>mSoundIfAwayChk</tabstop>
    <tabstop>configSound</tabstop>
</tabstops>
<layoutdefaults spacing="6" margin="11"/>
</UI>
