<!DOCTYPE UI><UI version="3.2" stdsetdef="1">
<class>BehaviorConfig_Chat</class>
<widget class="QWidget">
    <property name="name">
        <cstring>BehaviorConfig_Chat</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>552</width>
            <height>661</height>
        </rect>
    </property>
    <property name="caption">
        <string>Chat</string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QCheckBox">
            <property name="name">
                <cstring>cb_RaiseMsgWindowChk</cstring>
            </property>
            <property name="text">
                <string>&amp;Raise window/tab on new messages</string>
            </property>
        </widget>
        <widget class="QCheckBox">
            <property name="name">
                <cstring>cb_ShowEventsChk</cstring>
            </property>
            <property name="text">
                <string>Show e&amp;vents in chat window</string>
            </property>
        </widget>
        <widget class="QCheckBox">
            <property name="name">
                <cstring>highlightEnabled</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>7</hsizetype>
                    <vsizetype>0</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string>Highlight messages containing your &amp;nickname</string>
            </property>
        </widget>
        <widget class="QButtonGroup">
            <property name="name">
                <cstring>interfaceGroup</cstring>
            </property>
            <property name="title">
                <string>&amp;Interface Preference</string>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>mSingleShot</cstring>
                    </property>
                    <property name="text">
                        <string>Single-shot window (Jabber email/Old ICQ style)</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>mChatWindow</cstring>
                    </property>
                    <property name="text">
                        <string>Chat window (Jabber chat/MSN/New ICQ style)</string>
                    </property>
                </widget>
            </vbox>
        </widget>
        <widget class="QButtonGroup">
            <property name="name">
                <cstring>chatWindowGroup</cstring>
            </property>
            <property name="title">
                <string>Chat Window &amp;Grouping Policy</string>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>mNewWindow</cstring>
                    </property>
                    <property name="text">
                        <string>Open all messages in a new chat window</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Every chat has its own window.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>mTabAccountWindow</cstring>
                    </property>
                    <property name="text">
                        <string>Group messages from the same account in the same chat window</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>All chats for one account get grouped into one window by using tabs.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>mTabWindow</cstring>
                    </property>
                    <property name="text">
                        <string>Group all messages in the same chat window</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>All chats get grouped into one window by using tabs.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>mTabGroup</cstring>
                    </property>
                    <property name="text">
                        <string>Group messages from contacts in the same group in the same chat window</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>mTabMetaContact</cstring>
                    </property>
                    <property name="text">
                        <string>Group messages from the same metacontact in the same chat window</string>
                    </property>
                </widget>
            </vbox>
        </widget>
        <widget class="QLayoutWidget">
            <property name="name">
                <cstring>layout2</cstring>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>txtChatViewBufferSize</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Maximum chat window lines:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>mChatViewBufferSize</cstring>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Limit the maximum number of lines visible in a chat window to improve speed for complex layouts.</string>
                    </property>
                </widget>
                <widget class="QSpinBox">
                    <property name="name">
                        <cstring>mChatViewBufferSize</cstring>
                    </property>
                    <property name="maxValue">
                        <number>9000</number>
                    </property>
                    <property name="minValue">
                        <number>2</number>
                    </property>
                    <property name="value">
                        <number>250</number>
                    </property>
                </widget>
            </hbox>
        </widget>
        <spacer>
            <property name="name">
                <cstring>spacer2</cstring>
            </property>
            <property name="orientation">
                <enum>Vertical</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>20</width>
                    <height>300</height>
                </size>
            </property>
        </spacer>
    </vbox>
</widget>
<tabstops>
    <tabstop>cb_RaiseMsgWindowChk</tabstop>
    <tabstop>cb_ShowEventsChk</tabstop>
    <tabstop>highlightEnabled</tabstop>
    <tabstop>mSingleShot</tabstop>
    <tabstop>mChatWindow</tabstop>
    <tabstop>mNewWindow</tabstop>
    <tabstop>mTabAccountWindow</tabstop>
    <tabstop>mTabWindow</tabstop>
    <tabstop>mChatViewBufferSize</tabstop>
</tabstops>
<layoutdefaults spacing="6" margin="11"/>
</UI>
