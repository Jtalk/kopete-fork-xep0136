<!DOCTYPE UI><UI version="3.2" stdsetdef="1">
<class>HistoryPrefsUI</class>
<author>Olivier Goffart</author>
<widget class="QWidget">
    <property name="name">
        <cstring>Form1</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>459</width>
            <height>216</height>
        </rect>
    </property>
    <property name="caption">
        <string>Form1</string>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <spacer row="4" column="2">
            <property name="name">
                <cstring>spacer1</cstring>
            </property>
            <property name="orientation">
                <enum>Vertical</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>20</width>
                    <height>60</height>
                </size>
            </property>
        </spacer>
        <widget class="QCheckBox" row="0" column="0" rowspan="1" colspan="4">
            <property name="name">
                <cstring>Auto_chatwindow</cstring>
            </property>
            <property name="text">
                <string>Show messages from previous chats in new chat windows:</string>
            </property>
            <property name="checked">
                <bool>true</bool>
            </property>
        </widget>
        <widget class="QLayoutWidget" row="3" column="0" rowspan="1" colspan="4">
            <property name="name">
                <cstring>layout2</cstring>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>textLabel4</cstring>
                    </property>
                    <property name="text">
                        <string>Number of messages to show per page when browsing history:</string>
                    </property>
                </widget>
                <widget class="KIntSpinBox">
                    <property name="name">
                        <cstring>Number_ChatWindow</cstring>
                    </property>
                    <property name="value">
                        <number>20</number>
                    </property>
                </widget>
            </hbox>
        </widget>
        <spacer row="1" column="0">
            <property name="name">
                <cstring>spacer3</cstring>
            </property>
            <property name="orientation">
                <enum>Horizontal</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>60</width>
                    <height>20</height>
                </size>
            </property>
        </spacer>
        <spacer row="2" column="0">
            <property name="name">
                <cstring>spacer4</cstring>
            </property>
            <property name="orientation">
                <enum>Horizontal</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>60</width>
                    <height>20</height>
                </size>
            </property>
        </spacer>
        <widget class="QLabel" row="1" column="1">
            <property name="name">
                <cstring>numberLabel</cstring>
            </property>
            <property name="text">
                <string>Number of messages to show:</string>
            </property>
            <property name="buddy" stdset="0">
                <cstring>Number_Auto_chatwindow</cstring>
            </property>
        </widget>
        <widget class="QLabel" row="2" column="1">
            <property name="name">
                <cstring>colorLabel</cstring>
            </property>
            <property name="text">
                <string>Color of messages:</string>
            </property>
            <property name="buddy" stdset="0">
                <cstring>History_color</cstring>
            </property>
        </widget>
        <widget class="KColorButton" row="2" column="2">
            <property name="name">
                <cstring>History_color</cstring>
            </property>
            <property name="text">
                <string></string>
            </property>
            <property name="color">
                <color>
                    <red>170</red>
                    <green>170</green>
                    <blue>127</blue>
                </color>
            </property>
        </widget>
        <widget class="KIntSpinBox" row="1" column="2">
            <property name="name">
                <cstring>Number_Auto_chatwindow</cstring>
            </property>
            <property name="value">
                <number>7</number>
            </property>
        </widget>
        <spacer row="1" column="3">
            <property name="name">
                <cstring>spacer5</cstring>
            </property>
            <property name="orientation">
                <enum>Horizontal</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>101</width>
                    <height>20</height>
                </size>
            </property>
        </spacer>
        <spacer row="2" column="3">
            <property name="name">
                <cstring>spacer6</cstring>
            </property>
            <property name="orientation">
                <enum>Horizontal</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>91</width>
                    <height>20</height>
                </size>
            </property>
        </spacer>
    </grid>
</widget>
<customwidgets>
</customwidgets>
<connections>
    <connection>
        <sender>Auto_chatwindow</sender>
        <signal>toggled(bool)</signal>
        <receiver>Number_Auto_chatwindow</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>Auto_chatwindow</sender>
        <signal>toggled(bool)</signal>
        <receiver>History_color</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>Auto_chatwindow</sender>
        <signal>toggled(bool)</signal>
        <receiver>colorLabel</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>Auto_chatwindow</sender>
        <signal>toggled(bool)</signal>
        <receiver>numberLabel</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>Auto_chatwindow</tabstop>
    <tabstop>Number_Auto_chatwindow</tabstop>
    <tabstop>History_color</tabstop>
    <tabstop>Number_ChatWindow</tabstop>
</tabstops>
<layoutdefaults spacing="6" margin="11"/>
<includehints>
    <includehint>knuminput.h</includehint>
    <includehint>kcolorbutton.h</includehint>
    <includehint>knuminput.h</includehint>
</includehints>
</UI>
