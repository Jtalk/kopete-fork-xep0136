<!DOCTYPE UI><UI version="3.1" stdsetdef="1">
<class>HistoryPrefsUI</class>
<author>Olivier Goffart</author>
<widget class="QWidget">
    <property name="name">
        <cstring>Form1</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>383</width>
            <height>226</height>
        </rect>
    </property>
    <property name="caption">
        <string></string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>0</number>
        </property>
        <widget class="QLayoutWidget">
            <property name="name">
                <cstring>layout7</cstring>
            </property>
            <grid>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <spacer row="2" column="3">
                    <property name="name">
                        <cstring>spacer3</cstring>
                    </property>
                    <property name="orientation">
                        <enum>Horizontal</enum>
                    </property>
                    <property name="sizeType">
                        <enum>Expanding</enum>
                    </property>
                    <property name="sizeHint">
                        <size>
                            <width>8</width>
                            <height>20</height>
                        </size>
                    </property>
                </spacer>
                <widget class="QCheckBox" row="0" column="0" rowspan="1" colspan="2">
                    <property name="name">
                        <cstring>newView</cstring>
                    </property>
                    <property name="text">
                        <string>Old messages shown in new chat windows</string>
                    </property>
                </widget>
                <widget class="KIntSpinBox" row="2" column="2">
                    <property name="name">
                        <cstring>nbChatWindow</cstring>
                    </property>
                </widget>
                <widget class="QLabel" row="1" column="0">
                    <property name="name">
                        <cstring>textLabel1</cstring>
                    </property>
                    <property name="text">
                        <string>Color for old messages:</string>
                    </property>
                </widget>
                <widget class="QLabel" row="2" column="0" rowspan="1" colspan="2">
                    <property name="name">
                        <cstring>textLabel4</cstring>
                    </property>
                    <property name="text">
                        <string>Number of messages to show per page when browsing history:</string>
                    </property>
                </widget>
                <widget class="KIntSpinBox" row="0" column="2">
                    <property name="name">
                        <cstring>nbNewView</cstring>
                    </property>
                </widget>
                <widget class="KColorButton" row="1" column="1" rowspan="1" colspan="2">
                    <property name="name">
                        <cstring>m_color</cstring>
                    </property>
                    <property name="text">
                        <string></string>
                    </property>
                </widget>
            </grid>
        </widget>
        <spacer>
            <property name="name">
                <cstring>spacer1</cstring>
            </property>
            <property name="orientation">
                <enum>Vertical</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>16</width>
                    <height>16</height>
                </size>
            </property>
        </spacer>
        <widget class="QLabel">
            <property name="name">
                <cstring>textLabel2</cstring>
            </property>
            <property name="text">
                <string>&lt;qt&gt;&lt;b&gt;Warning&lt;/b&gt;: this plugin is not stable yet, please don't report bugs now.&lt;br&gt;
&lt;b&gt;Todo&lt;/b&gt;:
&lt;ul&gt;&lt;li&gt;add a selection of differents accounts here&lt;/li&gt;
&lt;li&gt;change the format the a small one, with more data&lt;/li&gt;
&lt;li&gt;improve the history dialog&lt;/li&gt;&lt;/ul&gt;
Help is welcome</string>
            </property>
        </widget>
    </vbox>
</widget>
<connections>
    <connection>
        <sender>newView</sender>
        <signal>toggled(bool)</signal>
        <receiver>nbNewView</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
</connections>
<layoutdefaults spacing="6" margin="11"/>
<includehints>
    <includehint>knuminput.h</includehint>
    <includehint>knuminput.h</includehint>
    <includehint>kcolorbutton.h</includehint>
</includehints>
</UI>
