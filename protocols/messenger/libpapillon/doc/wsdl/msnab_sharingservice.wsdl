<definitions targetNamespace="http://www.msn.com/webservices/AddressBook/"
			 xmlns:msnab="http://www.msn.com/webservices/AddressBook/"
			 xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
			 xmlns="http://schemas.xmlsoap.org/wsdl/">
	
	<types>
		<schema xmlns="http://www.w3.org/2001/XMLSchema">
			<import namespace="http://www.msn.com/webservices/AddressBook/"   schemaLocation="msnab_datatypes.xsd" />
			<import namespace="http://www.msn.com/webservices/AddressBook/" schemaLocation="msnab_servicetypes.xsd" />
		</schema>
	</types>
	
	<message name="FindMembership">
		<part name="ABHeader" element="msnab:ABHeaderType"/>
		<part name="body" element="msnb:FindMembershipRequestType"/>
	</message>
	
	<message name="FindMembershipResponse">
		<part name="ServiceHeader" element="msnab:ServiceHeaderType" />
		<part name="body" element="msnab:FindMembershipResultType" />
	</message>
	
	<portType name="FindMembershipPortType">
		<operation name="FindMembership">
			<input message="msnab:FindMembership" />
			<output message="msnab:FindMembershipResponse" />
		</operation>
	</portType>
	
	<binding name="FindMembershipSoap" type="FindMembershipPortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<operation name="FindMembership">
			<soap:operation soapAction="http://www.msn.com/webservices/AddressBook/FindMembership" />
			<input message="msnab:FindMembership">
				<soap:header message="msnab:FindMembership" part="ABHeader" use="literal"/>
				<soap:body parts="body" use="literal"/>
			</input>
			<output message="msnab:FindMembershipResponse">
				<soap:header message="msnab:FindMembershipResponse" part="ServiceHeader" use="literal" />
				<soap:body parts="body" use="literal" />
			</output>
		</operation>
	</binding>
	
	<service name="SharingService">
		<port name="FindMembershipPort" binding="msnab:FindMembershipSoap">
			<soap:address location="https://omega.contacts.msn.com/abservice/SharingService.asmx" />
		</port>
	</service>
</definitions>
